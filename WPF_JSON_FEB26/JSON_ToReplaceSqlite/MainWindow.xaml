<Window x:Class="JSON_ToReplaceSqlite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_ToReplaceSqlite"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,-13,0.4,-0.2">
        <Button x:Name="button" Content="Load Appointments" HorizontalAlignment="Left" Margin="191,64,0,0" VerticalAlignment="Top" Width="123" Click="button_Click"/>
        <Button x:Name="button1" Content="Select Customer" HorizontalAlignment="Left" Margin="191,142,0,0" VerticalAlignment="Top" Width="123"/>
        <Button x:Name="button2" Content="Save to CRM" HorizontalAlignment="Left" Margin="191,224,0,0" VerticalAlignment="Top" Width="123" Click="button2_Click"/>
        <TextBox x:Name="numFenceTextBox" HorizontalAlignment="Left" Height="23" Margin="388,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="numFenceLabel" Content="Number of Fences" HorizontalAlignment="Left" Margin="388,33,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="totalPriceLabel" Content="Label" HorizontalAlignment="Left" Margin="388,142,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
